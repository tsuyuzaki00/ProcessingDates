float Y;
void setup(){
  size(500,500,P3D);
  background(255,255,255);
  frameRate(10);
}

void draw(){
  background(255);
  Y = Y + 0.01;
  translate(250,250,0);
  rotateX(150);
  rotateY(Y);
  box(100,100,100);
}

void keyPressed(){
  if(key == 'x'){
    fill(0,255,0);
  }else if(key == 'z'){
    fill(255,0,0);
  }else if(key == 'c'){
    fill(0,0,255);
}
}
