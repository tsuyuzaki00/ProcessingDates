void setup(){
  size(300,300);
  background(255);
  fill(0);
  textSize(30);
  text("touch game",60,120);
  textSize(50);
  text("START",70,180);
}

void draw(){
  
}

void mousePressed(){
  if(mouseButton == LEFT){
      background(255);
      fill(255,0,0);
      stroke(255,0,0);
      rect(random(1,250),random(1,250),50,50);
  }
}
